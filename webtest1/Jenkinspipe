@Library("webtestlib") _
pipeline{
    agent{label "slave-linux"}
    stages{
          stage("Maven build"){
            steps{
                   sh 'mvn -f webtest1/pom.xml clean package'
            
                }
         }
         stage("deploy to tomcat"){
                                    
              steps{
                    tomcatdeploy('ec2-user','172.31.1.31','tomcat')
               
               }
          }
       }
  }
                      
